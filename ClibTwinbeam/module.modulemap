/*  module.modulemap | for later harmonization C++ and Swift. */

module ClibTwinbeam { requires macos 
  
  module Arm_Simd { requires neon 
    header "/Library/Developer/CommandLineTools/usr/lib/clang/13.0.0/include/arm_neon.h"
  }
  
  module Intel_Simd { requires /* sse41, */ sse 
    header "/Library/Developer/CommandLineTools/usr/lib/clang/13.0.0/include/mmintrin.h"
    header "/Library/Developer/CommandLineTools/usr/lib/clang/13.0.0/include/xmmintrin.h"
  }
  
  header "header.h"
  header "account-italian.h"

}

module ClibTwinbeam_and_Cpp {
  requires cplusplus
  header "header.h"
  header "account-italian.h"
  header "after-nine.hpp"
}

